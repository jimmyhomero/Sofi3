	<parameter name="inicio" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fin" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[ SELECT usuarios.`USU_NOMBRES` AS usuarios_USU_NOMBRES,             
     (SELECT SUM(factura.FAC_SUBTOTAL) FROM factura  WHERE( factura.FAC_ID_USUARIO =usuarios.USU_ID AND (factura.FAC_FECHA BETWEEN $P{inicio} AND $P{fin} )  )ORDER BY factura.FAC_SUBTOTAL ASC ) AS ventas ,
     (SELECT SUM(factura.FAC_iva) FROM factura  WHERE( factura.FAC_ID_USUARIO =usuarios.USU_ID AND (factura.FAC_FECHA BETWEEN $P{inicio} AND $P{fin} )  )ORDER BY factura.FAC_SUBTOTAL ASC ) AS IVA ,
     (SELECT SUM(factura.FAC_MONTO) FROM factura  WHERE( factura.FAC_ID_USUARIO =usuarios.USU_ID AND (factura.FAC_FECHA BETWEEN $P{inicio} AND $P{fin} )  )ORDER BY factura.FAC_SUBTOTAL ASC ) AS MONTO ,
(SELECT COUNT(factura.FAC_MONTO) FROM factura  WHERE( factura.FAC_ID_USUARIO =usuarios.USU_ID AND (factura.FAC_FECHA BETWEEN $P{inicio} AND $P{fin})  )ORDER BY factura.FAC_SUBTOTAL ASC ) AS NUMERO 
     FROM usuarios WHERE usuarios.USU_VISIBLE =1  ORDER BY usuarios.USU_NOMBRES     
     
     ]]>
	</queryString>
